{
  "name": "leovie/cronjob-validator",
  "authors": [
    {
      "name": "Leo Viezens",
      "role": "Developer"
    }
  ],
  "config": {
    "bin-dir": "bin",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "autoload": {
    "psr-4": {
      "LeoVie\\CronjobValidator\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LeoVie\\CronjobValidator\\Test\\": "tests"
    }
  },
  "require-dev": {
    "infection/infection": "^0.13.6",
    "phpstan/phpstan-phpunit": "^0.11.2",
    "phpstan/phpstan-shim": "^0.11.16",
    "phpunit/phpunit": "^8.3"
  },
  "require": {
  }
}
